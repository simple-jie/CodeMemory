<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.simple_jie.codememory">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".presentation.home.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.dagger.DaggerSimpleActivity"
            android:label="@string/dagger_simple">
        </activity>
        <activity
            android:name=".presentation.dagger.DaggerSimple2Activity"
            android:label="@string/dagger_simple2">
        </activity>
        <activity android:name=".presentation.dagger2.SubcomponentActivity"
            android:label="@string/dagger_subcomponent">
        </activity>
    </application>

</manifest>